<div class="series">
	
	<h1>Series</h1>

	<div class="row" id="seriesIndex">
    	<% @series.each do |series| %>
    		<div class="col-xs-6 col-sm-2 col-md-2 col-lg-2 seriesTitle">
       			<h3><%= link_to(series.title, series) %></h3>
        	</div>
    	<% end %>
	</div>
	<br>

	<%= link_to 'New Series', new_series_path %>
</div>
